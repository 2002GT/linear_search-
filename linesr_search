#include <iostream>

using namespace std;

 int linearSearch( int arr[], int n , int key){
   for( int i = 0 ; i<=n-1; i++){
       if( key==arr[i]){
           return i;
       }
       
   }
       return -1;
 }
   int main()
{
    // PROGRAM OF LINEAR SEARCH
    int n;
    cin>>n;
    
    int arr[n];
    
    for(int i=0; i<=n-1 ; i++){
        cin>>arr[i];
    }
       int key;
       cin>>key;
    
    //  calling the function
    
    cout<<linearSearch( arr , n , key )<<endl;

    return 0;
}

